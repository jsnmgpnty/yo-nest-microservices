{
  "name": "<%= name %>",
  "version": "0.0.1",
  "description": "<%= description %>",
  "private": true,
  "license": "UNLICENSED",
  "author": "<%= author %>",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build && npm run copy:assets",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "copy:assets": "cpx './src/public/**' 'dist/public'",
    "start": "npm run copy:assets && nest start",
    "start:dev": "npm run copy:assets && nest start --watch",
    "start:debug": "npm run copy:assets && nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json"
  },
  "dependencies": {
    "@azure/storage-blob": "^12.2.0-preview.1",
    "@elastic/elasticsearch": "^7.9.0",
    "@golevelup/nestjs-rabbitmq": "^1.15.1",
    "@nestjs/common": "^7.4.2",
    "@nestjs/core": "^7.0.0",
    "@nestjs/microservices": "^7.4.2",
    "@nestjs/mongoose": "^7.0.2",
    "@nestjs/platform-express": "^7.0.0",
    "@nestjs/platform-fastify": "^7.4.2",
    "@nestjs/serve-static": "^2.1.3",
    "@nestjs/swagger": "^4.6.0",
    "amqp-connection-manager": "^3.2.0",
    "amqplib": "^0.6.0",
    "cache-manager": "^3.3.0",
    "dotenv": "^8.2.0",
    "fastify": "^3.2.1",
    "fastify-helmet": "^5.0.1",
    "fastify-swagger": "^3.3.0",
    "lodash": "^4.17.20",
    "moment": "^2.27.0",
    "mongoose": "^5.10.0",
    "redis": "^3.0.2",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^6.5.4",
    "swagger-client": "^3.10.12",
    "winston": "^3.3.3",
    "winston-elasticsearch": "^0.10.0"
  },
  "devDependencies": {
    "@nestjs/cli": "^7.0.0",
    "@nestjs/schematics": "^7.0.0",
    "@nestjs/testing": "^7.4.2",
    "@types/amqplib": "^0.5.13",
    "@types/express": "^4.17.3",
    "@types/jest": "^25.2.3",
    "@types/lodash": "^4.14.159",
    "@types/mongoose": "^5.7.36",
    "@types/node": "^13.9.1",
    "@types/redis": "^2.8.26",
    "@types/supertest": "^2.0.8",
    "@types/winston": "^2.4.4",
    "@typescript-eslint/eslint-plugin": "3.0.2",
    "@typescript-eslint/parser": "3.0.2",
    "cpx": "^1.5.0",
    "eslint": "7.1.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-plugin-import": "^2.20.1",
    "into-stream": "^6.0.0",
    "jest": "26.0.1",
    "prettier": "^1.19.1",
    "supertest": "^4.0.2",
    "ts-jest": "26.1.0",
    "ts-loader": "^6.2.1",
    "ts-node": "^8.6.2",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^3.7.4"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
